 -- DA_HOOD
    local DA_HOOD = Window:NewTab("DA HOOD")
    local DA_HOODSection = DA_HOOD:NewSection("DA HOOD SCRIPTS")
    
    DA_HOODSection:NewButton("INF HEALTH", "ITS JUST YOUR REGULAR SCRIPT", function()
        local Player = game.Players.LocalPlayer
        Player.Character.Humanoid.Health = math.huge
    end)
    
    DA_HOODSection:NewButton("SUPER JUMP", "ITS JUST YOUR REGULAR SCRIPT", function()
        local Player = game.Players.LocalPlayer
        Player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        Player.Character.Humanoid.JumpPower = 100
    end)
    
    DA_HOODSection:NewButton("FLY", "ITS JUST YOUR REGULAR SCRIPT", function()
        local Player = game.Players.LocalPlayer
        Player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        wait()
        Player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Freefall)
    end)


 --Arsenal
    local Arsenal = Window:NewTab("Arsenal")
    local ArsenalSection = Arsenal:NewSection("Arsenal scripts")
    
    ArsenalSection:NewButton("Aimbot", "gives you aimbot", function()
        local Players = game:GetService("Players")
    
        local function Aimbot()
            for _, player in ipairs(Players:GetPlayers()) do
                if player ~= Players.LocalPlayer and player.Character and player.Character:FindFirstChild("Head") then
                    local head = player.Character.Head
                    local target = head.Position + Vector3.new(0, 1, 0)
                    
                    game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.Position, target)
                end
            end
        end
        
        while true do
            wait(0.1)
            Aimbot()
        end
    end)
    
    ArsenalSection:NewButton("inf damage", "gives you inf damage", function()
        local DamageValue = math.huge
    
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(DamageValue)
    end)


