-- Load the library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/AlterX404/GUI/main/base%20UI%20functions"))()

-- Create the window
local Window = Library.CreateLib("Alter-Hub", "BloodTheme")

-- CREDITS
local Other = Window:NewTab("CREDITS")
local OtherSection = Other:NewSection("CREDITS")
local OtherSection = Other:NewSection("ALL CREDITS GO TO ALTER-X#5868")
OtherSection:NewButton("GO SUBSCRIBE TO ALTER-X ON YOUTUBE FOR MORE SCRIPTS", "SUBSCRIBE NOW!!!!!!!", function()
    local link = "https://www.youtube.com/channel/UCBTkrkGEibTUayqVvs8G5fg"
    -- Open link in default browser
    syn.open_url(link)
end)

-- LOCAL PLAYER
local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")

PlayerSection:NewSlider("Walkspeed", "SPEED!!", 500, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlayerSection:NewSlider("Jumppower", "JUMP HIGH!!", 350, 50, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpHeight = s
end)

PlayerSection:NewButton("Reset WS/JP", "Resets to all defaults", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpHeight = 50
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)

