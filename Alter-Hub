local Library = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()
local Window = Library:Window("Alter-Hub")
win:SetImage("https://i.ibb.co/P1HJJpk/download.png")

-- LOCAL PLAYER
local Player = Window:Server("Alter-Hub", "")
local PlayerSection = Player:Channel("Player")

local walkspeedSlider = playerSection:Slider("Walkspeed", 16, 500, 200, function(value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)

playerSection:Button("Set Walkspeed to 50", function()
    walkspeedSlider:Change(50)
end)

DiscordLib:Notification("Notification", "Alter-hub GUI loaded!", "Okay!")
